<%= link_to "Home", root_path %>
<h1> <%= @skill.name %> </h1>
<%= link_to "Create Roadmap", new_roadmap_path %><br>
<% @skill.roadmaps.each do |roadmap| %>
	<%= link_to roadmap.title, roadmap_path(roadmap.id) %><br>
	<%= roadmap.description %> <br>
	vote = <%= roadmap.votes.size %> - <%=button_to "Upvote", vote_roadmap_path(roadmap), method: :put %><br>
<% end %>