<% photo = @profile.image %>
Image:<image src=<%= "#{@profile.image}" %> /><br>

Username: <%= @profile.username %><br>
Twitter: <%= @profile.twitter_name %><br>
Github: <%= @profile.github_name %><br>
Learning Style: <%= @profile.learning_style %><br>
Bio: <%= @profile.bio %><br>

<% if current_user == @profile %>
<%= link_to "Edit Profile", edit_profile_path(@profile) %><br>
<%= link_to "Account Settings", edit_user_registration_path %><br>
<% end %>

<%= link_to "Create Roadmap", new_roadmap_path %><br>
<br>
<% @profile.roadmaps.each do |roadmap| %>
	<%= link_to roadmap.title, roadmap_path(roadmap) %><br>
<% end %>